{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/manifest.json", "!/ngsw-worker.js"],
        "versionedFiles": ["/styles.*.css", "/main.*.js", "/runtime.*.js", "/polyfills.*.js"],
        "urls": [
          "https://fonts.googleapis.com/icon?family=Material+Icons",
          "https://fonts.googleapis.com/css?family=Roboto:300,400,500",
          "https://fonts.gstatic.com/**"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "fixtures-api",
      "urls": ["http://api.football-data.org/v1/competitions/467/fixtures"],
      "cacheConfig": {
        "strategy": "performance",
        "maxAge": "100d",
        "maxSize": 10
      }
    },
    {
      "name": "flags",
      "urls": ["http://www.countryflags.io/**"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxAge": "100d",
        "maxSize": 100
      }
    }
  ]
}
